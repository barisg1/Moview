@{
}



@model Moview.Models.Users

@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <h2>Profil Informations</h2>
    
    <dl class="row">
        <dt class="col-sm-2">Username:</dt>
        <dd class="col-sm-10">@Model.Username</dd>
        
        <dt class="col-sm-2">Name:</dt>
        <dd class="col-sm-10">@Model.FirstName</dd>
        
        <dt class="col-sm-2">Surname:</dt>
        <dd class="col-sm-10">@Model.LastName</dd>
        
        <dt class="col-sm-2">E-mail:</dt>
        <dd class="col-sm-10">@Model.Email</dd>
    </dl>
    
    <a asp-action="Edit" class="btn btn-primary">Edit Profile</a>
</div>
